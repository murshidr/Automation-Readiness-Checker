<svg viewBox="0 0 800 800" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="grad1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0EA5E9;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2563EB;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="grad2" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#6366F1;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#8B5CF6;stop-opacity:1" />
    </linearGradient>
    <filter id="shadow">
      <feDropShadow dx="0" dy="4" stdDeviation="8" flood-opacity="0.15"/>
    </filter>
  </defs>
  
  <!-- Background Circle -->
  <circle cx="400" cy="400" r="380" fill="#0F172A" opacity="0.03"/>
  
  <!-- Main Stack Structure -->
  <g filter="url(#shadow)">
    <!-- Bottom Layer -->
    <path d="M 280 480 L 400 520 L 520 480 L 520 440 L 400 480 L 280 440 Z" 
          fill="url(#grad1)" opacity="0.9"/>
    
    <!-- Middle Layer -->
    <path d="M 260 400 L 400 450 L 540 400 L 540 360 L 400 410 L 260 360 Z" 
          fill="url(#grad1)" opacity="0.95"/>
    
    <!-- Top Layer -->
    <path d="M 240 320 L 400 380 L 560 320 L 560 280 L 400 340 L 240 280 Z" 
          fill="url(#grad2)"/>
    
    <!-- Left Face Bottom -->
    <path d="M 280 440 L 280 480 L 400 520 L 400 480 Z" 
          fill="#1E40AF" opacity="0.6"/>
    
    <!-- Left Face Middle -->
    <path d="M 260 360 L 260 400 L 400 450 L 400 410 Z" 
          fill="#1E40AF" opacity="0.7"/>
    
    <!-- Left Face Top -->
    <path d="M 240 280 L 240 320 L 400 380 L 400 340 Z" 
          fill="#4F46E5" opacity="0.8"/>
    
    <!-- Right Face Bottom -->
    <path d="M 400 480 L 400 520 L 520 480 L 520 440 Z" 
          fill="#0C4A6E" opacity="0.6"/>
    
    <!-- Right Face Middle -->
    <path d="M 400 410 L 400 450 L 540 400 L 540 360 Z" 
          fill="#0C4A6E" opacity="0.7"/>
    
    <!-- Right Face Top -->
    <path d="M 400 340 L 400 380 L 560 320 L 560 280 Z" 
          fill="#3730A3" opacity="0.8"/>
  </g>
  
  <!-- Quantum Symbol -->
  <g transform="translate(400, 340)">
    <!-- Electron Orbit Rings -->
    <ellipse cx="0" cy="0" rx="50" ry="15" fill="none" stroke="#38BDF8" stroke-width="2.5" opacity="0.8"/>
    <ellipse cx="0" cy="0" rx="50" ry="15" fill="none" stroke="#38BDF8" stroke-width="2.5" opacity="0.8" transform="rotate(60)"/>
    <ellipse cx="0" cy="0" rx="50" ry="15" fill="none" stroke="#38BDF8" stroke-width="2.5" opacity="0.8" transform="rotate(120)"/>
    
    <!-- Nucleus -->
    <circle cx="0" cy="0" r="8" fill="#60A5FA"/>
    <circle cx="0" cy="0" r="5" fill="#93C5FD"/>
  </g>
  
  <!-- Decorative Circuit Lines -->
  <g opacity="0.3">
    <line x1="180" y1="350" x2="220" y2="350" stroke="#0EA5E9" stroke-width="2"/>
    <circle cx="180" cy="350" r="3" fill="#0EA5E9"/>
    <circle cx="220" cy="350" r="3" fill="#0EA5E9"/>
    
    <line x1="580" y1="350" x2="620" y2="350" stroke="#6366F1" stroke-width="2"/>
    <circle cx="580" cy="350" r="3" fill="#6366F1"/>
    <circle cx="620" cy="350" r="3" fill="#6366F1"/>
  </g>
  
  <!-- Text -->
  <text x="400" y="620" font-family="'Inter', 'Segoe UI', system-ui, sans-serif" 
        font-size="56" font-weight="700" fill="#0F172A" text-anchor="middle">
    Quantumstacks
  </text>
  <text x="400" y="670" font-family="'Inter', 'Segoe UI', system-ui, sans-serif" 
        font-size="42" font-weight="400" fill="#6366F1" text-anchor="middle">
    Lab
  </text>
  
  <!-- Subtle corner brackets -->
  <g opacity="0.2" stroke="#2563EB" stroke-width="3" fill="none">
    <path d="M 120 120 L 120 80 L 160 80"/>
    <path d="M 680 120 L 680 80 L 640 80"/>
    <path d="M 120 680 L 120 720 L 160 720"/>
    <path d="M 680 680 L 680 720 L 640 720"/>
  </g>
</svg>